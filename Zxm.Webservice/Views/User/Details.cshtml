@{
    ViewBag.Title = "User Details";
}

<h2>@ViewBag.User.FirstName @ViewBag.User.LastName</h2>

<table>
    @foreach (var userProperty in ViewBag.UserProperties)
    {
        <tr>
            <td style="font-weight: bold;">@userProperty.Key</td>
            <td style="padding-left: 20px;">@userProperty.Value</td>
        </tr>
    }
</table>